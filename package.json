{
	"name": "@mariozechner/pi",
	"version": "0.3.0",
	"description": "CLI tool for managing vLLM deployments on GPU pods",
	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"pi": "dist/cli.js"
	},
	"scripts": {
		"build": "tsc && cp src/models.json dist/",
		"dev": "tsc --watch --preserveWatchOutput",
		"clean": "rm -rf dist",
		"prepublishOnly": "npm run clean && npm run build",
		"check": "biome check --write . && tsc --noEmit",
		"prepare": "husky"
	},
	"files": [
		"dist/**/*",
		"README.md",
		"LICENSE"
	],
	"keywords": [
		"llm",
		"vllm",
		"gpu",
		"ai",
		"cli"
	],
	"author": "Mario Zechner",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/badlogic/pi.git"
	},
	"bugs": {
		"url": "https://github.com/badlogic/pi/issues"
	},
	"homepage": "https://github.com/badlogic/pi#readme",
	"engines": {
		"node": ">=18.0.0"
	},
	"preferGlobal": true,
	"dependencies": {
		"@ai-sdk/openai": "^2.0.5",
		"ai": "^5.0.8",
		"chalk": "^5.5.0",
		"openai": "^5.12.1",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.1.3",
		"@types/node": "^20.19.9",
		"husky": "^9.1.7",
		"tsx": "^4.20.3",
		"typescript": "^5.9.2"
	},
	"types": "./dist/index.d.ts"
}
